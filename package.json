{
  "name": "mia-wtf",
  "version": "0.0.1",
  "description": "Starting over on a personal site with 11ty 2.0",
  "main": "index.html",
  "scripts": {
    "watch:sass": "sass --watch src/_sass:_site/css",
    "watch:eleventy": "eleventy --serve --incremental",
    "build:sass": "sass src/_sass:_site/css",
    "build:eleventy": "eleventy",
    "start": "ELEVENTY_ENV=dev npm-run-all build:sass --parallel watch:*",
    "build": "ELEVENTY_ENV=prod npm-run-all build:sass build:eleventy",
    "lint": "stylelint \"**/*.scss\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mirisuzanne/mia.wtf.git"
  },
  "keywords": [],
  "author": "mirisuzanne",
  "license": "ISC",
  "private": true,
  "bugs": {
    "url": "https://github.com/mirisuzanne/mia.wtf/issues"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0-beta.3",
    "@11ty/eleventy-fetch": "^3.0.0",
    "@11ty/eleventy-img": "^3.0.0",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.4.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.48.0",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-scss": "^4.1.0"
  },
  "dependencies": {
    "netlify-plugin-cache": "^1.0.3"
  }
}
