<script webc:setup>
  const cite = (data) => {
    const src = data.at
      ? `[${data.venue || data.credit}](${data.at})`
      : data.venue || data.credit;

    return data.venue && data.credit
      ? `${data.credit}, ${src}`
      : src;
  };
</script>

<figure
  webc:for="item of this.fromData"
  webc:if="item.text"
  is-quote
>
  <blockquote @html="md(item.text)"></blockquote>
  <figcaption
    cite-quote
    @html="mdI(`---${cite(item)}`)"
  ></figcaption>
</figure>
