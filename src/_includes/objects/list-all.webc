<section
  webc:if="this.collection && withTag(this.collection, 'is:post').length > 0"
  list-of="posts"
>
  <h2>Articles &c.</h2>
  <post-list
    :@collection="this.collection"
  ></post-list>
</section>

<section
  webc:if="this.collection && withTag(this.collection, 'is:artifact').length > 0"
  list-of="artifacts"
>
  <h2>Artifacts</h2>
  <artifact-list
    :@collection="this.collection"
  ></artifact-list>
</section>
