<li
  webc:root="override"
  class="h-card p-org"
>
  <a
    :href="this.org.url"
    class="p-name u-url"
    @text="pageName(this.org)"
  ></a>

  <date-range
    :@date="this.org.date"
    :@end="this.org.data.end"
  ></date-range>

  <p-note
    class="p-note"
    @html="`(${mdI(this.org.data.sub)})`"
  ></p-note>
</div>
